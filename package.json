{
  "name": "mono-redux-starter",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "update": "nx migrate latest",
    "start:template": "nx serve template",
    "build:template": "nx build template --skip-nx-cache --prod",
    "run:many": "nx run-many --target=build --projects=template --prod",
    "lint:template": "nx lint --project=template",
    "lint": "nx workspace-lint && nx lint",
    "lint:all": "nx run-many --target=lint --all --maxWarnings=0",
    "lint:all:fix": "nx run-many --target=lint --all --fix --maxWarnings=0",
    "e2e:template": "nx e2e template-e2e --browser chrome --watch",
    "extract-messages": "formatjs extract apps/**/*.{ts,tsx} libs/**/*.{ts,tsx,json} --out-file libs/shared/i18n/en.json --format formatter.js",
    "test:templateapp": "nx test templateapp --watch --updateSnapshot",
    "test:template": "nx test template --watch --updateSnapshot",
    "test:ci": "nx run-many --target=test --all --parallel=5 --maxParallel=10 --updateSnapshot --ci --clearCache"
  },
  "private": true,
  "dependencies": {
    "@casl/ability": "^6.1.1",
    "@casl/react": "^3.1.0",
    "@emotion/react": "^11.10.4",
    "@emotion/styled": "^11.10.4",
    "@mui/icons-material": "^5.10.3",
    "@mui/material": "^5.10.3",
    "@reduxjs/toolkit": "^1.8.5",
    "axios": "^0.27.2",
    "core-js": "^3.6.5",
    "crypto-js": "^4.1.1",
    "dayjs": "^1.11.5",
    "formik": "^2.2.9",
    "jwt-decode": "^3.1.2",
    "next-redux-wrapper": "^7.0.5",
    "nookies": "^2.5.2",
    "notistack": "^2.0.5",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-helmet-async": "^1.3.0",
    "react-intl": "^6.1.0",
    "react-number-format": "^4.9.4",
    "react-redux": "^8.0.2",
    "react-router-dom": "^6.3.0",
    "redux-logger": "^3.0.6",
    "redux-persist-transform-encrypt": "^4.0.0",
    "reduxjs-toolkit-persist": "^7.2.1",
    "regenerator-runtime": "0.13.7",
    "tslib": "^2.3.0",
    "universal-cookie": "^4.0.4",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@nrwl/cli": "15.4.5",
    "@nrwl/cypress": "15.4.5",
    "@nrwl/eslint-plugin-nx": "15.4.5",
    "@nrwl/jest": "15.4.5",
    "@nrwl/linter": "15.4.5",
    "@nrwl/nx-cloud": "latest",
    "@nrwl/react": "15.4.5",
    "@nrwl/web": "15.4.5",
    "@nrwl/workspace": "15.4.5",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.10",
    "@svgr/webpack": "^6.5.1",
    "@testing-library/react": "13.3.0",
    "@types/crypto-js": "^4.1.1",
    "@types/jest": "28.1.1",
    "@types/node": "16.11.7",
    "@types/react": "18.0.18",
    "@types/react-dom": "18.0.6",
    "@types/redux-logger": "^3.0.9",
    "@typescript-eslint/eslint-plugin": "~5.33.1",
    "@typescript-eslint/parser": "~5.33.1",
    "babel-jest": "28.1.1",
    "cypress": "^10.2.0",
    "eslint": "~8.15.0",
    "eslint-config-prettier": "8.1.0",
    "eslint-plugin-cypress": "^2.10.3",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-jsx-a11y": "6.6.1",
    "eslint-plugin-react": "7.31.1",
    "eslint-plugin-react-hooks": "4.6.0",
    "jest": "28.1.1",
    "jest-environment-jsdom": "28.1.1",
    "nx": "15.4.5",
    "prettier": "^2.6.2",
    "react-test-renderer": "18.2.0",
    "ts-jest": "28.0.5",
    "ts-node": "10.9.1",
    "typescript": "~4.7.2"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not op_mini all"
  ]
}
