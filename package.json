{
  "name": "mono-redux-starter",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "update": "nx migrate latest",
    "start:template": "nx serve template --port=4201",
    "build:template": "nx build template --skip-nx-cache --prod",
    "run:many": "nx run-many --target=build --projects=template --prod",
    "lint:template": "nx lint --project=template",
    "lint": "nx workspace-lint && nx lint",
    "lint:all": "nx run-many --target=lint --all --maxWarnings=0",
    "lint:all:fix": "nx run-many --target=lint --all --fix --maxWarnings=0",
    "e2e:template": "nx e2e template-e2e --browser chrome --watch",
    "extract-messages": "formatjs extract apps/**/*.{ts,tsx} libs/**/*.{ts,tsx,json} --out-file libs/shared/i18n/en.json --format formatter.js",
    "test:templateapp": "nx test templateapp --watch --updateSnapshot",
    "test:template": "nx test template --watch --updateSnapshot",
    "test:ci": "nx run-many --target=test --all --parallel=5 --maxParallel=10 --updateSnapshot --ci --clearCache"
  },
  "private": true,
  "dependencies": {
    "@casl/ability": "^6.1.1",
    "@casl/react": "^3.1.0",
    "@emotion/react": "11.11.1",
    "@emotion/styled": "11.11.0",
    "@mui/icons-material": "^5.10.3",
    "@mui/material": "^5.10.3",
    "@reduxjs/toolkit": "1.9.3",
    "axios": "^0.27.2",
    "core-js": "^3.6.5",
    "crypto-js": "^4.1.1",
    "dayjs": "^1.11.5",
    "formik": "^2.2.9",
    "jwt-decode": "^3.1.2",
    "next-redux-wrapper": "^7.0.5",
    "notistack": "^2.0.5",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-intl": "^6.1.0",
    "react-number-format": "^4.9.4",
    "react-redux": "^8.0.2",
    "react-router-dom": "^6.14.2",
    "redux-logger": "^3.0.6",
    "redux-persist-transform-encrypt": "^4.0.0",
    "reduxjs-toolkit-persist": "^7.2.1",
    "regenerator-runtime": "0.13.7",
    "tslib": "^2.3.0",
    "universal-cookie": "^4.0.4",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@nrwl/js": "16.6.0",
    "@nx/cypress": "16.6.0",
    "@nx/eslint-plugin": "16.6.0",
    "@nx/jest": "16.6.0",
    "@nx/linter": "16.6.0",
    "@nx/react": "16.6.0",
    "@nx/web": "16.6.0",
    "@nx/webpack": "^16.6.0",
    "@nx/workspace": "16.6.0",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.10",
    "@svgr/webpack": "^6.5.1",
    "@testing-library/react": "14.0.0",
    "@types/crypto-js": "^4.1.1",
    "@types/jest": "29.4.4",
    "@types/node": "18.14.2",
    "@types/react": "18.2.14",
    "@types/react-dom": "18.2.6",
    "@types/redux-logger": "^3.0.9",
    "@typescript-eslint/eslint-plugin": "5.60.1",
    "@typescript-eslint/parser": "5.60.1",
    "babel-jest": "29.4.3",
    "cypress": "^10.2.0",
    "eslint": "~8.15.0",
    "eslint-config-prettier": "8.1.0",
    "eslint-plugin-cypress": "^2.10.3",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-jsx-a11y": "6.7.1",
    "eslint-plugin-react": "7.32.2",
    "eslint-plugin-react-hooks": "4.6.0",
    "jest": "29.4.3",
    "jest-environment-jsdom": "29.4.3",
    "nx": "16.6.0",
    "prettier": "^2.6.2",
    "ts-jest": "29.1.1",
    "ts-node": "10.9.1",
    "typescript": "~4.7.2"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not op_mini all"
  ]
}
